{
  "name": "@steerprotocol/stop-loss-lp-strategy",
  "version": "0.0.1",
  "description": "AssemblyScript based strategy template for the Steer Protocol",
  "main": "index.js",
  "scripts": {
    "test": "yarn jest",
    "build:debug": "asc assembly/index.ts --target debug --runPasses asyncify --exportRuntime --runtime stub",
    "build:release": "asc assembly/index.ts --target release --runPasses asyncify --exportRuntime --runtime stub",
    "start": "npx serve .",
    "docs": "typedoc --tsconfig ./tsconfig.json",
    "strategy": "yarn build:debug && yarn test",
    "semantic-release": "semantic-release",
    "commit": "cz",
    "postinstall": "yarn build:debug && yarn build:release"
  },
  "author": "Derek Barrera <derekbarrera@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@babel/preset-typescript": "^7.23.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@steerprotocol/base-strategy": "^0.1.0",
    "@steerprotocol/concentrated-liquidity-strategy": "1.0.8",
    "@steerprotocol/strategy-utils": "^3.1.2",
    "as-bignum": "^0.3.0",
    "commitizen": "^4.2.4",
    "conventional-changelog-conventionalcommits": "^7.0.2",
    "json-as": "^0.7.0",
    "serve": "^14.2.1",
    "typescript": "^5.3.3",
    "visitor-as": "^0.11.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.7",
    "@steerprotocol/app-loader": "^1.0.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/node-fetch": "^2.6.10",
    "assemblyscript": "^0.27.22",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.7.0",
    "cz-conventional-changelog": "3.3.0",
    "jest": "^29.7.0",
    "regenerator-runtime": "^0.14.1",
    "semantic-release": "^22.0.12",
    "typedoc": "^0.25.6"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
